{
    "semi": true,
    "singleQuote": true,
    "singleAttributePerLine": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "tabWidth": 4,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "endOfLine": "lf"
